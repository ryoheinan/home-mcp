{
	"extends": "@tsconfig/recommended/tsconfig.json",
	"compilerOptions": {
		"target": "ES2022",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"lib": ["ES2022"],
		"types": ["vitest/globals"],
		"skipLibCheck": true,
		"noEmit": true,
		"noUncheckedIndexedAccess": true
	},
	"include": ["src/**/*.ts", "worker-configuration.d.ts"]
}
