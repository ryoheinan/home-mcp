{
	"name": "home-mcp",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "wrangler dev",
		"deploy": "wrangler deploy",
		"typegen": "pnpm run gen:cf-types && pnpm run gen:nature-remo-types",
		"gen:cf-types": "wrangler types --env-interface CloudflareBindings",
		"gen:nature-remo-types": "openapi-typescript https://swagger.nature.global/swagger.json -o src/types/generated/nature-remo-openapi.ts",
		"test": "vitest run",
		"test:watch": "vitest --watch",
		"typecheck": "tsc --noEmit",
		"lint": "biome check .",
		"fix": "biome check --write ."
	},
	"dependencies": {
		"@hono/mcp": "^0.2.3",
		"@modelcontextprotocol/sdk": "^1.26.0",
		"hono": "^4.11.8",
		"openapi-fetch": "^0.15.0",
		"zod": "^4.3.6"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.14",
		"@tsconfig/recommended": "^1.0.13",
		"openapi-typescript": "^7.10.1",
		"typescript": "^5.9.3",
		"vitest": "^4.0.14",
		"wrangler": "^4.63.0"
	},
	"packageManager": "pnpm@10.28.2+sha512.41872f037ad22f7348e3b1debbaf7e867cfd448f2726d9cf74c08f19507c31d2c8e7a11525b983febc2df640b5438dee6023ebb1f84ed43cc2d654d2bc326264"
}
